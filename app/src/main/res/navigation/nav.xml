<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.marvelapp.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_splash2Fragment"
            app:destination="@id/splash2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/splash2Fragment"
        android:name="com.example.marvelapp.Splash2Fragment"
        android:label="fragment_splash2"
        tools:layout="@layout/fragment_splash2" >
        <action
            android:id="@+id/action_splash2Fragment_to_loginFragment2"
            app:destination="@id/loginFragment2" />
        <action
            android:id="@+id/action_splash2Fragment_to_createFragment"
            app:destination="@id/createFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment2"
        android:name="com.example.marvelapp.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment2_to_planFragment"
            app:destination="@id/planFragment" />
        <action
            android:id="@+id/action_loginFragment2_to_createFragment"
            app:destination="@id/createFragment" />
        <action
            android:id="@+id/action_loginFragment2_to_menuHomeFragment"
            app:destination="@id/menuHomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/createFragment"
        android:name="com.example.marvelapp.CreateFragment"
        android:label="fragment_create"
        tools:layout="@layout/fragment_create" >
        <action
            android:id="@+id/action_createFragment_to_loginFragment2"
            app:destination="@id/loginFragment2" />
        <action
            android:id="@+id/action_createFragment_to_planFragment"
            app:destination="@id/planFragment" />
    </fragment>
    <fragment
        android:id="@+id/planFragment"
        android:name="com.example.marvelapp.PlanFragment"
        android:label="fragment_plan"
        tools:layout="@layout/fragment_plan" >
        <action
            android:id="@+id/action_planFragment_to_bankFragment"
            app:destination="@id/bankFragment" />
    </fragment>
    <fragment
        android:id="@+id/bankFragment"
        android:name="com.example.marvelapp.BankFragment"
        android:label="fragment_bank"
        tools:layout="@layout/fragment_bank" >
        <action
            android:id="@+id/action_bankFragment_to_cardFragment"
            app:destination="@id/cardFragment" />
    </fragment>
    <fragment
        android:id="@+id/cardFragment"
        android:name="com.example.marvelapp.CardFragment"
        android:label="fragment_card"
        tools:layout="@layout/fragment_card" >
        <action
            android:id="@+id/action_cardFragment_to_menuHomeFragment"
            app:destination="@id/menuHomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/informaFragment"
        android:name="com.example.marvelapp.InformaFragment"
        android:label="fragment_informa"
        tools:layout="@layout/fragment_informa" />
    <fragment
        android:id="@+id/menuHomeFragment"
        android:name="com.example.marvelapp.MenuHomeFragment"
        android:label="fragment_menu_home"
        tools:layout="@layout/fragment_menu_home" >
        <action
            android:id="@+id/action_menuHomeFragment_to_informaFragment"
            app:destination="@id/informaFragment" />
        <action
            android:id="@+id/action_menuHomeFragment_to_loginFragment2"
            app:destination="@id/loginFragment2" />
        <action
            android:id="@+id/action_menuHomeFragment_to_loginFragment22"
            app:destination="@id/loginFragment2" />
        <action
            android:id="@+id/action_menuHomeFragment_to_cardFragment"
            app:destination="@id/cardFragment" />
        <action
            android:id="@+id/action_menuHomeFragment_to_reviewFragment"
            app:destination="@id/reviewFragment" />
    </fragment>
    <fragment
        android:id="@+id/reviewFragment"
        android:name="com.example.marvelapp.ReviewFragment"
        android:label="fragment_review"
        tools:layout="@layout/fragment_review" />
</navigation>